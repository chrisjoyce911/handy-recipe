{
    "name": "Moroccan beef kebabs",
    "description": "description",
    "image": "https://via.placeholder.com/345x194/FFFF00",
    "prep": 5,
    "cooking": 5,
    "phase": [

        {
            "name": "Marinade",
            "ingredients": [
                {
                    "quanty": "1/2 cup",
                    "ingredient": "Chopped fresh coriander"
                },
                {
                    "quanty": "60ml (1/4 cup)",
                    "ingredient": "Lemon juice"
                },
                {
                    "quanty": "2 cloves",
                    "ingredient": "Garlic, crushed"
                },
                {
                    "quanty": "2 teaspoons",
                    "ingredient": "Ground coriander"
                },
                {
                    "quanty": "2 teaspoons",
                    "ingredient": "Olive oil"
                },
                {
                    "quanty": "2 teaspoons",
                    "ingredient": "Ground cumin"
                },
                {
                    "quanty": "1/2 teaspoon",
                    "ingredient": "Dried chilli flakes"
                },
                {
                    "quanty": "1/4 teaspoon",
                    "ingredient": "Ground cinnamon"
                },
                {
                    "quanty": "750g",
                    "ingredient": "beef porterhouse steak, trimmed, cut into 3cm pieces"
                }

            ]
        }
    ],
    "method": [
        {
            "order": 1,
            "description": "Place fresh coriander, lemon juice, garlic, oil, ground coriander, cumin, chilli flakes and cinnamon in a glass or ceramic bowl."
        },
        {
            "order": 2,
            "description": "Cut beef into 3cm pieces"
        },
        {
            "order": 3,
            "description": "Add the beef and toss to coat. Cover with plastic wrap and place in the fridge for 1 hour to marinate."
        },
        {
            "order": 4,
            "description": "Thread the beef onto bamboo skewers. Season with salt and pepper."
        },
        {
            "order": 5,
            "description": "Preheat a chargrill or barbecue grill on medium-high. Spray skewers with oil. Cook for 2-3 minutes each side for medium or until browned and cooked to your liking."
        },
        {
            "order": 6,
            "description": "Transfer to a plate and cover loosely with foil to keep warm. Set aside for 5 minutes to rest."
        }
    ],
    "notes": [
        {
            "order": 1,
            "note": "For Moroccan Lamb Kebabs, replace the beef with lamb leg steak and the fresh coriander with fresh mint."
        }
    ],
    "link": "https://www.taste.com.au/recipes/moroccan-beef-kebabs/5109776f-c61b-4fd4-b53b-5d3c15ba42f1"
}